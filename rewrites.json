[
	{
		"from": "db/*",
		"to": "../../*"
	},
	{
		"from": "db",
		"to": "../../"
	},
	{
		"from": "",
		"to": "index.html"
	},
	{
		"from": "mazes/:id/tiles.xml",
		"to": "_list/tiles_rss/tiles_by_creation",
		"query": {
			"id": ":id",
			"limit": "10",
			"descending": "true",
			"include_docs": "true"
		}
	},
	{
		"from": "mazes_static/:id",
		"to": "_list/maze_static/maze_and_tiles",
		"query": {
			"id": ":id",
			"descending": "true"
		}
	},
	{
		"from": "claims.xml",
		"to": "_list/claims_rss/claims",
		"query": {
			"limit": "10",
			"descending": "true"
		}
	},
	{
		"from": "flags.xml",
		"to": "_list/flags_rss/tile_flags",
		"query": {
			"limit": "10",
			"group": "true",
			"descending": "true"
		}
	},
	{
		"from": "favicon.ico",
		"to": "images/favicon.ico"
	},
	{
		"from": "*",
		"to": "*"
	}
]

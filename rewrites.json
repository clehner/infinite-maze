[
	{
		"from": "db/*",
		"to": "../../*"
	},
	{
		"from": "",
		"to": "_list/maze/maze_and_tiles",
		"query": {
			"currentdir": true,
			"id": "1",
			"key": "\"1\""
		}
	},
	{
		"from": "mazes/:id",
		"to": "_list/maze/maze_and_tiles",
		"query": {
			"id": ":id"
		}
	},
	{
		"from": "mazes/:id/tiles.xml",
		"to": "_list/tiles_rss/tiles_by_creation",
		"query": {
			"id": ":id",
			"limit": 10,
			"descending": true
		}
	},
	{
		"from": "mazes_static/:id",
		"to": "_list/maze_static/maze_and_tiles",
		"query": {
			"id": ":id"
		}
	},
	{
		"from": "favicon.ico",
		"to": "images/favicon.ico"
	},
	{
		"from": "*",
		"to": "*"
	}
]